{
    "version": 2,
    "buildCommand": "npm start",
    "installCommand": "npm install",
    "builds": [
        {
            "src": "src/main.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/main.ts",
            "methods": [
                "GET",
                "POST",
                "PATCH",
                "DELETE"
            ]
        }
    ],
}